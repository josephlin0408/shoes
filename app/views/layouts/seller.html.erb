<!DOCTYPE html>
<html>
<head>
  <title>Shoes</title>
  <%= stylesheet_link_tag    'seller', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'seller/sellercommon/top' %>
<%= render 'seller/sellercommon/menu' %>
<%= yield %>

<% if notice %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-danger"><%= alert %></p>
<% end %>

</body>
</html>
